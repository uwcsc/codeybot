    "engine-blackjack-ts": "^0.9.11",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "sqlite": "^4.0.22",
    "sqlite3": "^5.0.2",
    "typescript": "^4.2.4",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
